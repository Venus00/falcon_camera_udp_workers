{
  "name": "military-workers",
  "version": "1.0.0",
  "description": "Multi-service project: UDP Camera Workers + File Storage System",
  "private": true,
  "scripts": {
    "install:all": "cd udp_workers && npm install && cd ../stockage && npm install",
    "build:all": "cd udp_workers && npm run build && cd ../stockage && npm run build",
    "dev:udp": "cd udp_workers && npm run dev",
    "dev:storage": "cd stockage && npm run dev",
    "dev:all": "concurrently \"npm run dev:udp\" \"npm run dev:storage\"",
    "pelco:server": "cd udp_workers && npm run pelco:server",
    "pelco:terminal": "cd udp_workers && npm run pelco:terminal",
    "smart:server": "cd udp_workers && npm run smart:server",
    "smart:terminal": "cd udp_workers && npm run smart:terminal",
    "ftp": "cd stockage && npm run ftp",
    "api": "cd stockage && npm run api"
  },
  "keywords": ["udp", "ftp", "express", "pelco-d", "smart", "camera", "storage"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
